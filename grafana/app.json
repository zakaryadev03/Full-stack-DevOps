      "title": "HTTP Request Rate (per service, per route)",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": null
          },
          "refId": "A",
          "expr": "sum(rate(http_requests_total{namespace=\"sre-lab\"}[5m])) by (app, route)",
          "legendFormat": "{{app}} - {{route}}",
          "interval": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisCenteredZero": false,
